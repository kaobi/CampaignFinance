<p id="notice"><%= notice %></p>

<h1>Listing Candidates</h1>

<table>
  <thead>
    <tr>
      <th>State</th>
      <th>Prefix</th>
      <th>First name</th>
      <th>Mi</th>
      <th>Alias</th>
      <th>Last name</th>
      <th>Suffix</th>
      <th>Change address</th>
      <th>Address</th>
      <th>Appt suite num</th>
      <th>City</th>
      <th>Zip code</th>
      <th>Phone</th>
      <th>Extension</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @candidates.each do |candidate| %>
      <tr>
        <td><%= candidate.state_id %></td>
        <td><%= candidate.prefix_id %></td>
        <td><%= candidate.first_name %></td>
        <td><%= candidate.mi %></td>
        <td><%= candidate.alias %></td>
        <td><%= candidate.last_name %></td>
        <td><%= candidate.suffix %></td>
        <td><%= candidate.change_address %></td>
        <td><%= candidate.address %></td>
        <td><%= candidate.appt_suite_num %></td>
        <td><%= candidate.city %></td>
        <td><%= candidate.zip_code %></td>
        <td><%= candidate.phone %></td>
        <td><%= candidate.extension %></td>
        <td><%= link_to 'Show', candidate %></td>
        <td><%= link_to 'Edit', edit_candidate_path(candidate) %></td>
        <td><%= link_to 'Destroy', candidate, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Candidate', new_candidate_path %>
