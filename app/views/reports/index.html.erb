<p id="notice"><%= notice %></p>

<h1>Listing Reports</h1>

<table>
  <thead>
    <tr>
      <th>Filer</th>
      <th>Report type</th>
      <th>Submit date</th>
      <th>File name</th>
      <th>Origin</th>
      <th>Campaign year</th>
      <th>Upload type</th>
      <th>On time</th>
      <th>Validity</th>
      <th>Signature</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @reports.each do |report| %>
      <tr>
        <td><%= report.filer_id %></td>
        <td><%= report.report_type_id %></td>
        <td><%= report.submit_date %></td>
        <td><%= report.file_name %></td>
        <td><%= report.origin %></td>
        <td><%= report.campaign_year %></td>
        <td><%= report.upload_type %></td>
        <td><%= report.on_time %></td>
        <td><%= report.validity %></td>
        <td><%= report.signature %></td>
        <td><%= link_to 'Show', report %></td>
        <td><%= link_to 'Edit', edit_report_path(report) %></td>
        <td><%= link_to 'Destroy', report, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Report', new_report_path %>
