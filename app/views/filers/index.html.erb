<p id="notice"><%= notice %></p>

<h1>Listing Filers</h1>

<table>
  <thead>
    <tr>
      <th>User account</th>
      <th>Filer type</th>
      <th>State</th>
      <th>Office held</th>
      <th>Office sought</th>
      <th>First name</th>
      <th>Mi</th>
      <th>Last name</th>
      <th>Committee</th>
      <th>Address</th>
      <th>Secondary address</th>
      <th>Suite</th>
      <th>City</th>
      <th>Zip code</th>
      <th>Phone</th>
      <th>Extension</th>
      <th>Email</th>
      <th>Hand deliver</th>
      <th>Receive date</th>
      <th>Postmark</th>
      <th>Active</th>
      <th>Date created</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @filers.each do |filer| %>
      <tr>
        <td><%= filer.user_account_id %></td>
        <td><%= filer.filer_type_id %></td>
        <td><%= filer.state_id %></td>
        <td><%= filer.office_held_id %></td>
        <td><%= filer.office_sought_id %></td>
        <td><%= filer.first_name %></td>
        <td><%= filer.mi %></td>
        <td><%= filer.last_name %></td>
        <td><%= filer.committee %></td>
        <td><%= filer.address %></td>
        <td><%= filer.secondary_address %></td>
        <td><%= filer.suite %></td>
        <td><%= filer.city %></td>
        <td><%= filer.zip_code %></td>
        <td><%= filer.phone %></td>
        <td><%= filer.extension %></td>
        <td><%= filer.email %></td>
        <td><%= filer.hand_deliver %></td>
        <td><%= filer.receive_date %></td>
        <td><%= filer.postmark %></td>
        <td><%= filer.active %></td>
        <td><%= filer.date_created %></td>
        <td><%= link_to 'Show', filer %></td>
        <td><%= link_to 'Edit', edit_filer_path(filer) %></td>
        <td><%= link_to 'Destroy', filer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

