<p id="notice"><%= notice %></p>

<h1>Listing Treasurers</h1>

<table>
  <thead>
    <tr>
      <th>Address type</th>
      <th>State</th>
      <th>Prefix</th>
      <th>First name</th>
      <th>Mi</th>
      <th>Alias</th>
      <th>Last name</th>
      <th>Suffix</th>
      <th>Change address</th>
      <th>Address</th>
      <th>Suite</th>
      <th>City</th>
      <th>Zip code</th>
      <th>Phone</th>
      <th>Extension</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @treasurers.each do |treasurer| %>
      <tr>
        <td><%= treasurer.address_type_id %></td>
        <td><%= treasurer.state_id %></td>
        <td><%= treasurer.prefix %></td>
        <td><%= treasurer.first_name %></td>
        <td><%= treasurer.mi %></td>
        <td><%= treasurer.alias %></td>
        <td><%= treasurer.last_name %></td>
        <td><%= treasurer.suffix %></td>
        <td><%= treasurer.change_address %></td>
        <td><%= treasurer.address %></td>
        <td><%= treasurer.suite %></td>
        <td><%= treasurer.city %></td>
        <td><%= treasurer.zip_code %></td>
        <td><%= treasurer.phone %></td>
        <td><%= treasurer.extension %></td>
        <td><%= link_to 'Show', treasurer %></td>
        <td><%= link_to 'Edit', edit_treasurer_path(treasurer) %></td>
        <td><%= link_to 'Destroy', treasurer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Treasurer', new_treasurer_path %>
